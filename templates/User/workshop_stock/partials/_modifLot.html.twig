<div class="modal fade bd-example-modal-l" tabindex="-1" role="dialog" aria-labelledby="myExtraLargeModalLabel"
    aria-hidden="true" id="modalLots2">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title title" id="exampleModalLabel">Lot: </h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                {{form_start(formLots)}}
                    <div class="d-flex">
                        <div class="ml-2">
                            {{ form_row(formLots.number) }}
                        </div>
                        <div class="ml-2">
                            {{ form_row(formLots.dateExp) }}
                        </div>
                        <div class="ml-2">
                            {{ form_row(formLots.quantity) }}
                        </div>
                        <div class="ml-2">
                            {{ form_row(formLots.dateEnter) }}      
                        </div>
                        <div class="ml-2">
                            {{ form_row(formLots.place) }}
                        </div> 
                    </div>
                    <button type="submit" class="btn action_button btn-block mt-2" id="btnContact">Enregistrer</button>
                {{form_end(formLots)}}
            </div>
        </div>
    </div>
</div>